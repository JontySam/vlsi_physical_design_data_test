---
permalink: /sitemap.xml
layout: null
sitemap: false
---
<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
  <url>
    <loc>{{ site.url }}/</loc>
    <lastmod>{{ site.buildDate }}</lastmod>
    <priority>1.0</priority>
  </url>
  <url>
    <loc>{{ site.url }}/guides/</loc>
    <lastmod>{{ site.buildDate }}</lastmod>
    <priority>0.9</priority>
  </url>
  <url>
    <loc>{{ site.url }}/tracks/</loc>
    <lastmod>{{ site.buildDate }}</lastmod>
    <priority>0.85</priority>
  </url>
  <url>
    <loc>{{ site.url }}/about/</loc>
    <lastmod>{{ site.buildDate }}</lastmod>
    <priority>0.5</priority>
  </url>

{% for topic in topics %}
  <url>
    <loc>{{ site.url }}/guides/{{ topic.slug }}/</loc>
    <lastmod>{{ topic.updated or site.buildDate }}</lastmod>
    <priority>0.75</priority>
  </url>
{% endfor %}

{% for track in tracks %}
  <url>
    <loc>{{ site.url }}/tracks/{{ track.slug }}/</loc>
    <lastmod>{{ site.buildDate }}</lastmod>
    <priority>0.7</priority>
  </url>
{% endfor %}
</urlset>
